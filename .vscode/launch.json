{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "TS Debug",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "runtimeArgs": ["--nolazy", "--loader", "ts-node/esm/transpile-only"],
  
        "args": ["src/soap/server.ts"],
        
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart",
        "skipFiles": ["<node_internals>/**", "node_modules/**"]
      }
    ]
  }